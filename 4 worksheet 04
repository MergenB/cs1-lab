#1
def func1(*args):
    for i in args:
        print(i)

func1(1, 2, 3)
func1(4, 5)

#2
def sum(x):
  if x == 0:
    return 0
  else:
    return x + sum(x-1)
print(sum(10))  

#3
srt = "pynative"
print(srt[::2])

#4
list = [1,2,3,4,7]
def check(list):
  if list[0] == list [-1]:
   return True
 else:
   return False
print(check(list))

